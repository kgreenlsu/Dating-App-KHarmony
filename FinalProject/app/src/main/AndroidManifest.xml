<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kgreen.finalproject" >

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomePage"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewAccount"
            android:label="@string/title_activity_new_account"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Preferences"
            android:label="@string/title_activity_preferences"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".BioPage"
            android:label="@string/title_activity_bio_page"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".personalProfile"
            android:label="@string/title_activity_personal_profile"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Match"
            android:label="@string/title_activity_match"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SignIn"
            android:label="@string/title_activity_sign_in" >
        </activity>
        <activity
            android:name=".Pref2"
            android:label="@string/title_activity_pref2" >
        </activity>
        <activity
            android:name=".personalProfile2"
            android:label="@string/title_activity_presonal_profile2" >
        </activity>
    </application>

</manifest>
